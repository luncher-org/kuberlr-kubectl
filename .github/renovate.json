{
  "extends": [
    "github>rancher/renovate-config#release"
  ],
  "baseBranches": [
    "main",
    "/^release\\/v[0-9]+.x/",
    "renovate-test"
  ],
  "prHourlyLimit": 2,
  "packageRules": [
    {
      "groupName": "Kubectl Bumps",
      "groupSlug": "kubectl-deps",
      "matchDepNames": [
        "kubernetes/kubernetes"
      ]
    },
    {
      "matchBaseBranches": ["renovate-test"],
      "matchDepNames": [
        "kubernetes/kubernetes"
      ],
      "allowedVersions": ">=1.29.0 <1.33.0"
    }
  ]
}
